<%- include('./partials/header') %>

<h2>Search Results</h2>
<ul>
  <% searchResults.forEach(result => { %> <% if (result.imageUrl) { %>
  <a href="/videogames/<%= result._id %>">
    <img src="<%= result.imageUrl %>" alt="<%= result.title %> Image" />
  </a>
  <% } else { %>
  <p>No Image</p>
  <% } %>
  <br />
  <h3><%= result.title %></h3>
  : <% }) %>
</ul>

<%- include('./partials/footer') %>
